{
    "name": "@pendle/sdk-v2",
    "author": "Pendle Labs",
    "description": "SDK of Pendle Protocol.",
    "license": "MIT",
    "version": "0.0.37",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "homepage": "https://pendle.finance",
    "keywords": [
        "pendle",
        "sdk",
        "v2"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/pendle-finance/pendle-sdk-core-v2"
    },
    "bugs": {
        "url": "https://github.com/pendle-finance/pendle-sdk-core-v2/issues"
    },
    "scripts": {
        "format": "yarn prettier --write \"{src,test}/**/*.{ts,js}\"",
        "lint": "yarn eslint \"{src,test}/**/*.{ts,js}\" --fix",
        "check-types": "tsc --noemit --project tsconfig.json",
        "test": "yarn jest",
        "test:prepare": "yarn ts-node test/util/prepareMarket.ts && yarn test:approveRouter",
        "test:approveRouter": "yarn ts-node test/util/approveRouter.ts",
        "test:all": "yarn node test/all.js",
        "test:coverage": "yarn jest --coverage --maxWorkers=1",
        "build": "tsdx build --tsconfig tsconfig.build.json",
        "prepare": "tsdx build --tsconfig tsconfig.build.json",
        "clean": "rm -rf dist/"
    },
    "dependencies": {
        "@pendle/core-v2": "0.11.9-testnet",
        "axios": "^0.27.2",
        "bignumber.js": "^9.1.0",
        "ethers": "^5.7.0"
    },
    "devDependencies": {
        "@size-limit/preset-small-lib": "^8.0.1",
        "@types/jest": "^28.1.8",
        "@typescript-eslint/parser": "^5.35.1",
        "dotenv": "^16.0.1",
        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.1",
        "jest": "^28.1.3",
        "prettier": "^2.7.1",
        "size-limit": "^8.0.1",
        "ts-jest": "^28.0.8",
        "ts-node": "^10.9.1",
        "tsdx": "^0.14.1",
        "tslib": "^2.4.0",
        "typescript": "^4.7.4"
    },
    "resolutions": {
        "**/typescript": "^4.7.4"
    }
}
