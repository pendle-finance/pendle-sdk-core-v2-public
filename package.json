{
    "name": "@pendle/sdk-v2",
    "author": "Pendle Labs",
    "description": "SDK of Pendle Protocol.",
    "license": "MIT",
    "version": "0.0.60-weekly",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "homepage": "https://pendle.finance",
    "keywords": [
        "pendle",
        "sdk",
        "v2"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/pendle-finance/pendle-sdk-core-v2"
    },
    "bugs": {
        "url": "https://github.com/pendle-finance/pendle-sdk-core-v2/issues"
    },
    "scripts": {
        "format": "yarn prettier --write \"{src,test}/**/*.{ts,js}\"",
        "lint": "yarn eslint \"{src,test}/**/*.{ts,js}\" --fix",
        "check-types": "tsc --noemit --project tsconfig.json",
        "test": "yarn jest",
        "test:prepare": "yarn ts-node test/scripts/prepareMarket.ts && yarn test:approveRouter",
        "test:approveRouter": "yarn ts-node test/scripts/approveRouter.ts",
        "test:coverage": "yarn jest --coverage --maxWorkers=1",
        "build": "tsdx build --tsconfig tsconfig.build.json",
        "prepare": "yarn generateContractErrors && yarn build",
        "clean": "rm -rf dist/",
        "generateContractErrors": "yarn ts-node scripts/generatePendleContractErrorMessageHandler.ts"
    },
    "dependencies": {
        "@pendle/core-v2": "0.16.5-testnet",
        "axios": "^0.27.2",
        "bignumber.js": "^9.1.0",
        "ethers": "^5.7.1"
    },
    "devDependencies": {
        "@size-limit/preset-small-lib": "^8.1.0",
        "@types/jest": "^29.2.0",
        "@typescript-eslint/parser": "^5.40.1",
        "dotenv": "^16.0.3",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.1",
        "jest": "^29.2.1",
        "prettier": "^2.7.1",
        "size-limit": "^8.1.0",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "tsdx": "^0.14.1",
        "tslib": "^2.4.0",
        "typescript": "^4.8.4"
    },
    "resolutions": {
        "**/typescript": "^4.8.4"
    }
}
